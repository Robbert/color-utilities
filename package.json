{
  "name": "color-utilities",
  "version": "1.0.0",
  "description": "Library to format, convert and work with colors.",
  "main": "lib/index.js",
  "files": [
    "/lib"
  ],
  "scripts": {
    "test": "jest",
    "test-watch": "npx jest --watch",
    "coverage": "vitest run --coverage",
    "color-test": "tsc --build && node ./lib/tests/color-test.js",
    "color-test-to-file": "tsc --build && node ./lib/tests/color-test.js > color-test.txt",
    "blender-test": "tsc --build && node ./lib/tests/blender-test.js",
    "blender-test-to-file": "tsc --build && node ./lib/tests/blender-test.js > blender-test.txt",
