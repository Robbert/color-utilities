    "converter-test-to-file": "tsc --build && node ./lib/tests/converter-test.js > converter-test.txt",
    "harmony-test": "tsc --build && node ./lib/tests/harmony-test.js",
    "harmony-test-to-file": "tsc --build && node ./lib/tests/harmony-test.js > harmony-test.txt",
    "test-all": "npm run color-test > test.txt && npm run blender-test >> test.txt && npm run converter-test >> test.txt && npm run harmony-test >> test.txt"
  },
